{
	"cSpell.words": ["xyflow"],
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"eslint.enable": true,
	"eslint.run": "onSave",
	"eslint.validate": [
		"javascript",
		"javascriptreact",
		"typescript",
		"typescriptreact"
	],
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.requireConfig": true,
	"prettier.useEditorConfig": false,
	"editor.codeActionsOnSave": { "source.fixAll.eslint": "always" }
}
